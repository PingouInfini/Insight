<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Equipment.
    -->
    <changeSet id="20180920084601-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="equipment_id"
                                 baseTableName="equipment_location"
                                 constraintName="fk_equipment_location_equipment_id"
                                 referencedColumnNames="id"
                                 referencedTableName="equipment"/>
        <addForeignKeyConstraint baseColumnNames="locations_id"
                                 baseTableName="equipment_location"
                                 constraintName="fk_equipment_location_locations_id"
                                 referencedColumnNames="id"
                                 referencedTableName="location"/>
        
        <addForeignKeyConstraint baseColumnNames="equipment_id"
                                 baseTableName="equipment_organisation"
                                 constraintName="fk_equipment_organisation_equipment_id"
                                 referencedColumnNames="id"
                                 referencedTableName="equipment"/>
        <addForeignKeyConstraint baseColumnNames="organisations_id"
                                 baseTableName="equipment_organisation"
                                 constraintName="fk_equipment_organisation_organisations_id"
                                 referencedColumnNames="id"
                                 referencedTableName="organisation"/>
        
    </changeSet>
</databaseChangeLog>
